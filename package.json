{
  "name": "gas-fire",
  "version": "4.1.1-beta",
  "author": "Melle Dijkstra",
  "description": "Google App Script utilities to automate the FIRE google sheet",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": "24"
  },
  "scripts": {
    "prepare": "husky",
    "serve": "vite",
    "dev": "npm run build:dev && npm run publish && npm run serve",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "deploy": "npm run build && npm run publish",
    "publish": "clasp push -f",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "analyze:server": "echo 'not implemented yet'",
    "analyze:client": "echo 'not implemented yet'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ci:local": "CI=true npm run lint && npm run build && npm run test",
    "setup:https": "mkdir certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "switch-env": "tsx ./scripts/switch-env.ts",
    "faker": "tsx ./scripts/faker/faker.ts",
    "knip": "knip"
  },
  "dependencies": {
    "papaparse": "^5.5.3",
    "svelte": "^5.43.5",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@google/clasp": "^3.1.1",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/svelte-vite": "^10.0.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/google-apps-script": "^2.0.7",
    "@types/node": "^24.0.15",
    "@types/papaparse": "^5.5.0",
    "@vitest/coverage-v8": "^4.0.8",
    "dotenv": "^17.2.0",
    "eslint": "^9.39.1",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.26.0",
    "flowbite-svelte-icons": "^3.0.0",
    "gas-client": "^1.2.1",
    "gas-types-detailed": "^1.1.2",
    "husky": "^9.1.7",
    "knip": "^5.47.0",
    "storybook": "^10.0.6",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melledijkstra/gas-fire.git"
  },
  "keywords": [
    "google-apps-script",
    "typescript",
    "financial",
    "automation",
    "script",
    "sheet",
    "google"
  ],
  "bugs": {
    "url": "https://github.com/melledijkstra/gas-fire/issues"
  }
}